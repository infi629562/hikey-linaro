HIKEY_KERNEL_DIR=hikey-linaro

. ${ROOT_DIR}/${HIKEY_KERNEL_DIR}/build.config.common
. ${ROOT_DIR}/${HIKEY_KERNEL_DIR}/build.config.aarch64

KERNEL_DIR=${HIKEY_KERNEL_DIR}
BRANCH=android-hikey-linaro-4.14
DEFCONFIG=hikey960_defconfig

FILES="
arch/arm64/boot/Image.gz-dtb
arch/arm64/boot/dts/hisilicon/hi3660-hikey960.dtb
vmlinux
System.map
"

KERNEL_BINARY=Image.gz-dtb
KERNEL_CMDLINE="console=ttyAMA6 printk.devkmsg=on buildvariant=userdebug loglevel=15 androidboot.hardware=hikey960 androidboot.verifiedbootstate=orange no_console_suspend firmware_class.path=/vendor/firmware efi=noruntime init=/init androidboot.boot_devices=soc/ff3b0000.ufs"
BOOT_IMAGE_HEADER_VERSION=0
VENDOR_RAMDISK_BINARY=ramdisk.img
